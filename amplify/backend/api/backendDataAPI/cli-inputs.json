{
  "version": 1,
  "paths": {
    "/create-account": {
      "name": "/create-account",
      "lambdaFunction": "createAccount",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/login": {
      "name": "/login",
      "lambdaFunction": "userLogin",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/password-recovery": {
      "name": "/password-recovery",
      "lambdaFunction": "recoverPassword",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    },
    "/data/{col}": {
      "name": "/data/{col}",
      "lambdaFunction": "fetchData",
      "permissions": {
        "setting": "open"
      }
    }
  }
}